{
    "manifest_version"  :     2,
    "name"              :     "Temp Notes",
    "short_name"        :     "TempNotes",
    "version"           :     "1.0.0",
    "description"       :     "ChromeTempNotes.",
    "permissions" : [
        "contextMenus"
    ],
    "background" : {
        "scripts" : [ "scripts/background.js" ]
    },
    "content_scripts" : [ {
        "matches" : [ "<all_urls>" ],
        "css" : [ "css/custom.css" ],
        "js" : [
            "scripts/external/jquery-3.1.1.min.js", "scripts/external/jquery-ui.min.js", "scripts/injected.js"
        ]
    } ]
}
